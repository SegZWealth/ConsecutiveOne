 public int FindMaxConsecutiveOnes(int[] nums) {
        int curconsecutiveone=0;
        int preconsecutiveone=0;
        int count=0;
        for(int i=0; i<nums.Length; i++)
        {
            count++;
          if(count <= nums.Length-2)
          {
             if((nums[i]%2)==1 && (nums[i+1]%2)==1 )
             {
              curconsecutiveone++; 
             }
          }
          else if(nums.Length-1 == count && (nums[i]%2)==1)
          {
                curconsecutiveone++;
          }
          else
          {
               preconsecutiveone= curconsecutiveone;
               curconsecutiveone=0;
          }         
        }
        return preconsecutiveone>curconsecutiveone ? preconsecutiveone : curconsecutiveone;
    }
